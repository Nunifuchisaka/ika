<!DOCTYPE html>
<html lang="ja_JP">
<head>
<meta charset="UTF-8">
<title><%- @title %></title>
<meta name="viewport" content="width=device-width">
<link rel="stylesheet" href="assets/css/common.css">
</head>
<body id="top" class="theme-black-1">

<div class="l-wrapper-1">
  <header class="l-header-1">
    <div class="l-header-1__1">
      <h1 class="l-header-1__h1">CambriArms</h1>
    </div>
    <div class="l-header-1__2">
      <a href="#gnav" class="button-2 js-gnav-toggle"><span></span><span></span><span></span></a>
    </div>
  </header>
  <nav id="gnav" class="nav-1 is-close">
    <ul class="nav-1__items">
      <li class="nav-1__item">
        <a href="/">トップページ</a>
      </li>
      <li class="nav-1__item">
        <a href="#hoge">hoge</a>
      </li>
      <li class="nav-1__item">
        <a href="#fuga">fuga</a>
      </li>
    </ul>
  </nav>
  <div id="view" style="margin-top: 1em"></div>
  <div style="margin-top: 1em;text-align: center;">
    <button id="shuffle" class="button-1 button-1-l1" tabindex="9">シャッフル</button>
  </div>
  <footer class="l-footer-1">
    <p class="l-footer-1__copyright">
      <small>
        &copy; <a href="http://nunifuchisaka.com/">ぬにふちさか</a><br />Powered by ブキチくん
      </small>
    </p>
  </footer>
</div>


<div id="modal-1__overlay"></div>
<div id="modal-1__wrapper">
  <div id="modal-1">
    <div id="modal-1__contents"></div>
    <div id="modal-1__clone"></div>
  </div>
</div>

<script id="modal-1-template" type="text/template">
  <section class="section-1">
    <header class="section-1__header">
      <h1 class="section-1__h1">
        <%- name %>
      </h1>
    </header>
    <div class="section-1__contents">
      <%- ca.categorys_jp[category] %>
    </div>
  </section>
</script>


<script id="view-template" type="text/template">
  <div class="paragraphs-1">
    <p>最終更新&emsp;&ensp;<%- finalUpdateTime %>&emsp;<small><%- finalUpdateDate %></small></p>
  </div>
  <div id="weapons" class="list-1" style="margin-top: 1em">
    <ul class="list-1__items">
      <%= weaponsItemHTML %>
    </ul>
  </div>
</script>


<script id="weapons-item-template" type="text/template">
  <li id="player-<%- index %>" class="list-1__item">
    <div class="list-1__item__1 name">
      <input class="js-input input-1" type="text" name="name" value="<%- name %>" data-player="<%- index %>" tabindex="<%- index + 1 %>" placeholder="なまえ" />
    </div>
    <div class="list-1__item__2 weapon">
      <!-- <img src="assets/img/buki-<%- buki %>-tn.png" alt="" width="63" /> -->
      <a href="#buki-<%- buki %>" class="js-modal-1__trigger" data-buki-index="<%- buki %>">
        <%- buki_jp %>
      </a>
    </div>
  </li>
</script>

<script src="https://cdn.socket.io/socket.io-1.3.7.js"></script>
<script src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
<script src="assets/libs/jquery.easing.js"></script>
<script src="assets/libs/jquery.cookie.js"></script>
<script src="assets/libs/lodash.js"></script>
<script src="assets/libs/backbone.js"></script>
<script src="assets/libs/xdate.js"></script>
<script src="assets/js/data.js"></script>
<script src="assets/js/app.js"></script>
<script src="assets/js/common.js"></script>

<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-73224373-1', 'auto');
  ga('send', 'pageview');
</script>

</body>
</html>